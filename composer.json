{
  "name": "ben-rowan/vcsv-stream",
  "description": "Virtual CSV stream for creating CSV fixtures",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ben Rowan"
    }
  ],
  "require": {
    "php": "^8.0",
    "fakerphp/faker": "^1.20"
  },
  "require-dev": {
    "phpstan/phpstan": "@stable",
    "phpmd/phpmd" : "@stable",
    "phpunit/phpunit": "@stable",
    "rector/rector": "^0.18.10"
  },
  "autoload": {
    "psr-4": {
      "BenRowan\\VCsvStream\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BenRowan\\VCsvStream\\Tests\\": "tests/"
    }
  }
}
